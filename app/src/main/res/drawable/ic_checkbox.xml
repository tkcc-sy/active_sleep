<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="50dp"
          android:height="50dp"
          android:drawable="@drawable/ic_checked_square"
          android:state_checked="true"
          android:state_window_focused="false"/>

    <item android:width="50dp"
          android:height="50dp"
          android:drawable="@drawable/ic_uncheck_square"
          android:state_checked="false"
          android:state_window_focused="false"/>

    <item android:width="50dp"
          android:height="50dp"
          android:drawable="@drawable/ic_checked_square"
          android:state_checked="true"
          android:state_pressed="true"/>

    <item android:width="50dp"
          android:height="50dp"
          android:drawable="@drawable/ic_checked_square"
          android:state_checked="false"
          android:state_pressed="true"/>

    <item android:width="50dp"
          android:height="50dp"
          android:drawable="@drawable/ic_checked_square"
          android:state_checked="true"
          android:state_focused="true"/>

    <item android:width="50dp"
          android:height="50dp"
          android:drawable="@drawable/ic_uncheck_square"
          android:state_checked="false"
          android:state_focused="true"/>

    <item android:width="50dp"
          android:height="50dp"
          android:drawable="@drawable/ic_uncheck_square"
          android:state_checked="false"/>

    <item android:width="50dp"
          android:height="50dp"
          android:drawable="@drawable/ic_checked_square"
          android:state_checked="true"/>

</selector>